# Development environment variables
# Copy this file to .env.dev and fill in your actual values

APP_ENV=dev
APP_SECRET=your-development-secret-here

# Database - SQLite for development
DATABASE_URL="sqlite:///%kernel.project_dir%/var/data.db"

# Redis - Local development
REDIS_URL="redis://127.0.0.1:6379"

# Cloudflare R2 - Development bucket
CLOUDFLARE_R2_ENDPOINT="https://your-dev-account-id.r2.cloudflarestorage.com"
CLOUDFLARE_R2_REGION="auto"
CLOUDFLARE_R2_ACCESS_KEY="your-dev-access-key"
CLOUDFLARE_R2_SECRET_KEY="your-dev-secret-key"
CLOUDFLARE_R2_BUCKET="trello-dev-attachments"